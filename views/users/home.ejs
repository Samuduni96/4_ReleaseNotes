<%- include('../layouts/header') %> 
<h2>
    Home Page 
</h2>
<p>
    Welcome to the Home Page. <br> All users will directed to this page after logged in.
</p>
<h3>Projects:</h3>

<form action="/home" method="GET">
    <% projects.forEach( project => { %>
        <div><%= project.name %>
            <br>
            <%= project.description %>
        </div>
        <a href="/rel/<%= project.id %>">View</a>
        <br><br>
    <% }) %>
</form>

<a href="/new_project">Add New Project</a>
