<%- include('../layouts/header') %> 

<head>
    <link href="/styles2.css" rel="stylesheet">
</head>

<body>
    <section class="page-section">
        <div class="container px-4 px-lg-5">
            <div class="row gx-4 gx-lg-5 justify-content-center">
                <div class="col-lg-8 col-xl-6 text-center">
                    <h2 class="mt-0">Release Note</h2>
                    <hr class="divider" />
                </div>
            </div>
        </div>
    </section>

    <section id="services" class="services container">
        <form action="/rel/<%=project.id%>/<%=releasenote.id%>" method="GET">     
            <div class="row gy-4">
                    <div class="col-md-6-user" data-aos="fade-up" data-aos-delay="300">
                    <div class="service-box service-box-left orange">
                    <h3><%= releasenote.title %></h3>
                    <p><%= releasenote.releaseVersion %></p>
                    <p><%= releasenote.CreatedAt.toISOString().split('T')[0] %></p>
                    <p><%- releasenote.description %></p>
                    </div>
                </div>
            </div>

            <div class="text-center mb-3 fw-bolder ">
                <a href="/rel/<%=project.id%>">Back</a>
            </div>
        </form> 
    </section>
</body>